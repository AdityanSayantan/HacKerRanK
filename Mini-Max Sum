import java.io.*;
import java.math.*;
import java.security.*;
import java.text.*;
import java.util.*;
import java.util.concurrent.*;
import java.util.function.*;
import java.util.regex.*;
import java.util.stream.*;
import static java.util.stream.Collectors.joining;
import static java.util.stream.Collectors.toList;

class Result {

    /*
     * Complete the 'miniMaxSum' function below.
     *
     * The function accepts INTEGER_ARRAY arr as parameter.
     */

    public static void miniMaxSum(List<Integer> arr) {
    // Write your code here
       // Initialize variables
    long total = 0;          // Stores sum of all elements
    int min = arr.get(0);    // Initialize min with first element
    int max = arr.get(0);    // Initialize max with first element

    // Iterate through all elements in the list
    for (int i = 0; i < arr.size(); i++) {
        int num = arr.get(i);    // Get current element
        
        total += num;           // Add to running total
        
        // Update min if current element is smaller
        if (num < min) min = num;
        
        // Update max if current element is larger
        if (num > max) max = num;
    }

    // Calculate minimum sum (excluding the largest element)
    long minSum = total - max;
    
    // Calculate maximum sum (excluding the smallest element)
    long maxSum = total - min;

    // Print results in required format: "minSum maxSum"
    System.out.println(minSum + " " + maxSum);

        
     
    }

}

public class Solution {
    public static void main(String[] args) throws IOException {
        BufferedReader bufferedReader = new BufferedReader(new InputStreamReader(System.in));

        List<Integer> arr = Stream.of(bufferedReader.readLine().replaceAll("\\s+$", "").split(" "))
            .map(Integer::parseInt)
            .collect(toList());

        Result.miniMaxSum(arr);

        bufferedReader.close();
    }
}
